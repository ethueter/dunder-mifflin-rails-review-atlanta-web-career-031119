<h1>Doggie Details</h1>


<h3>Name:</h3>
<p><%= @dog.name %></p><br>
<h3>Breed:</h3>
<p><%= @dog.breed %></p><br>
<h3>Age:</h3>
<p><%= @dog.age %></p>
<h3>Employees:</h3>
<ul>
  <% @dog.employees.each do |employee|%>
  <li><%= link_to(employee.to_s, employee_path(employee))%></li>
  <% end %>
</ul>
